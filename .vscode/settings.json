{
    "spellright.language": [
        "es"
    ],
    "spellright.documentTypes": [
        "plaintext",
        "markdown",
        "vue",
        "blade"
    ],
    "spellright.parserByClass": {
        "blade": {
            "parser": "xml"
        }
    },
    "prettier.vueIndentScriptAndStyle": true,
    "prettier.useTabs": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
}
